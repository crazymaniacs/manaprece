{
	"name": "manaprece",
	"version": "1.0.0",
	"description": "Cool goods search engine\r It is very usefull project",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/crazymaniacs/manaprece.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/crazymaniacs/manaprece/issues"
	},
	"homepage": "https://github.com/crazymaniacs/manaprece#readme",
	"scripts": {
		"start": "node index.js",
		"postinstall":
			"webpack --color -p --progress --hide-modules --display-optimization-bailout",
		"heroku-prebuild": "npm install --dev",
		"build":
			"cross-env NODE_ENV=production webpack --color -p --progress --hide-modules --display-optimization-bailout",
		"start:dev": "webpack -d && webpack-dev-server --hot --inline",
		"test": "jest --no-cache"
	},
	"dependencies": {
		"react": "^16.2.0",
		"react-dom": "^16.2.0",
		"react-redux": "^5.0.6",
		"redux": "^3.7.2"
	},
	"engines": {
		"node": "9.3.0"
	},
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-jest": "^21.2.0",
		"babel-loader": "^7.1.2",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"clean-webpack-plugin": "^0.1.17",
		"cross-env": "^5.1.1",
		"css-loader": "^0.28.7",
		"express": "^4.16.2",
		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "^1.1.5",
		"html-loader": "^0.5.1",
		"html-webpack-plugin": "^2.30.1",
		"jest": "^21.2.1",
		"jest-css-modules": "^1.1.0",
		"node-sass": "^4.7.2",
		"prettier": "1.9.2",
		"raf": "^3.4.0",
		"sass-loader": "^6.0.6",
		"url-loader": "^0.6.2",
		"webpack": "^3.10.0",
		"webpack-dev-server": "^2.9.7"
	},
	"jest": {
		"globals": {
			"NODE_ENV": "test"
		},
		"moduleFileExtensions": ["js", "jsx", "json"],
		"setupFiles": ["raf/polyfill"],
		"moduleDirectories": ["node_modules", "src"],
		"moduleNameMapper": {
			"^.+\\.(css|scss|less)$": "jest-css-modules"
		}
	}
}
